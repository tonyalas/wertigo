  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "rules": {
          "eqeqeq": 0,
          "quotes": [
            "warn",
            "single"
          ],
          "jsx-quotes": [
            "warn",
            "prefer-single"
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
